    "authors": [
        {
            "name": "Petros Diveris",
            "email": "pdiveris@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/contracts": "5.0.*",
        "illuminate/support": "5.0.*",
        "nesbot/carbon": "~1.19"
    },
    "autoload": {
        "psr-0": {
            "Elucidate": "src/"
        }
